{
  "appId": "kr.2skydev.laas",
  "productName": "LAAS",
  "publish": [
    {
      "provider": "github",
      "owner": "2skydev",
      "repo": "LAAS"
    }
  ],
  "asar": true,
  "win": {
    "target": "nsis",
    "icon": "resources/icons/logo@256.ico"
  },
  "mac": {
    "target": "dmg",
    "icon": "resources/icons/logo@512.png"
  },
  "nsis": {
    "oneClick": true,
    "artifactName": "LAAS-Setup-${version}.${ext}",
    "uninstallDisplayName": "LAAS"
  },
  "extraResources": [
    "./resources/**"
  ],
  "directories": {
    "output": "release/"
  }
}